<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @team.name %>
</p>

<p>
  <b>Street:</b>
  <%= @team.street %>
</p>

<p>
  <b>Zip:</b>
  <%= @team.zip_code %>
</p>

<p>
  <b>City:</b>
  <%= @team.city %>
</p>

<p>
  <b>Distance:</b>
  <%= @team.distance %>
</p>
<% unless @team.bonuses.empty? %>
<h3>Bonuses for team</h3>
<ul>
  <% @team.bonuses.each do |bonus| %>
    <li><%= bonus.bonus_type %> <%=bonus.value %></li>
  <% end %>
</ul>
<% end %>
<%= link_to 'Edit', edit_team_path(@team) %> |
<%= link_to 'Back', teams_path %>
